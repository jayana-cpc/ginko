{
  "name": "GINKO",
  "description": "by Katherine Lee & Jayana Nanayakkara",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "main.html",
    "default_icon": "ginko_icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; font-src https://fonts.googleapis.com https://fonts.gstatic.com https://db.onlinewebfonts.com;"
  }
}
